{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
        {
            "src": "/ai/(.*)",
            "dest": "src/main.py"
        },
        {
            "src": "/ai",
            "dest": "src/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ]
}
